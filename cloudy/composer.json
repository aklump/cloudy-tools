{
  "name": "aklump/cloudy",
  "description": "A BASH Framework for PHP-Minded Developers.",
  "license": "BSD-3-Clause",
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/aklump/glob"
    }
  ],
  "require": {
    "symfony/yaml": "^3.4 || ^4 || ^5 || ^6",
    "justinrainbow/json-schema": "^5.2",
    "aklump/loft-lib": "^2.1",
    "jasny/dotkey": "^1.0",
    "ckr/arraymerger": "^2.0",
    "ext-json": "*",
    "aklump/glob": "^0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "AKlump\\Cloudy\\": "./dist/php/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AKlump\\Cloudy\\Tests\\": "./tests/"
    }
  }
}
